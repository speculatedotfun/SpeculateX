## Copy to `.env.local` for local dev, or use Vercel env vars.
## Testnet (BSC chainId 97)

NEXT_PUBLIC_NETWORK=testnet
NEXT_PUBLIC_CHAIN_ID=97

# WalletConnect / Reown (REQUIRED for good wallet coverage)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Core contracts (override defaults in `lib/contracts.ts`)
NEXT_PUBLIC_TESTNET_CORE=0x...
NEXT_PUBLIC_TESTNET_USDC=0x...
NEXT_PUBLIC_TESTNET_TREASURY=0x...
NEXT_PUBLIC_TESTNET_RESOLVER=0x...
NEXT_PUBLIC_TESTNET_ADMIN=0x...

# Diamond facets (optional overrides)
NEXT_PUBLIC_TESTNET_FACET_MARKET=0x...
NEXT_PUBLIC_TESTNET_FACET_TRADING=0x...
NEXT_PUBLIC_TESTNET_FACET_LIQUIDITY=0x...
NEXT_PUBLIC_TESTNET_FACET_SETTLEMENT=0x...

# RPCs (optional overrides)
NEXT_PUBLIC_RPC_URL=https://bsc-testnet.publicnode.com
NEXT_PUBLIC_TESTNET_RPC_URL=https://bsc-testnet.publicnode.com

# Subgraph
NEXT_PUBLIC_GOLDSKY_HTTP_URL=https://api.goldsky.com/.../speculate-core-v2/...
NEXT_PUBLIC_GOLDSKY_TESTNET_HTTP_URL=https://api.goldsky.com/.../speculate-core-v2/...


